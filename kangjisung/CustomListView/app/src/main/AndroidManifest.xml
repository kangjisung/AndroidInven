<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.locallistview"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.home7.customlistview.LocalListviewActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.home7.customlistview.noticeListView"/>
        <activity android:name="com.example.home7.customlistview.ItemView" android:theme="@android:style/Theme.Dialog" />
        <activity android:name="com.example.home7.customlistview.EditDialog" android:theme="@android:style/Theme.Dialog" ></activity>
        <activity android:name="com.example.home7.customlistview.Stamp" android:theme="@android:style/Theme.Dialog"></activity>
        <activity android:name="com.example.home7.customlistview.Addcustom" android:theme="@android:style/Theme.Dialog"></activity>
        <Dialog android:name="com.example.home7.customlistview.DatePickerDialog" android:theme="@android:style/Theme.Dialog" />
    </application>

</manifest>
